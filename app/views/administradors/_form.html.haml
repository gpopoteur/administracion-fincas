= form_for @administrador do |f|
  - if @administrador.errors.any?
    #error_explanation
      %h2= "#{pluralize(@administrador.errors.count, "error")} prohibited this administrador from being saved:"
      %ul
        - @administrador.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :nombre
    = f.text_field :nombre
  .actions
    = f.submit 'Save'
